<!-- 头部 -->
<%- include('./blocks/header', { siteTitle: themeConfig.siteName }) %>
<!--头部 over-->

        <!-- 侧导航 -->
        <%- include('./blocks/nav', {tags: site.tags, menu: 'archives', link: '/archives'}) %>
        <!--侧导航 over-->

        <!-- 顶导航 -->
        <%- include('./blocks/head') %>
        <!--顶导航 over-->

        <!-- 内容 -->
<section class="pcoded-main-container" id="content">
    <div class="pcoded-content">
        <div class="row">
            <div class="col-md-8">
                <!-- [ 公告 ] start -->
                <%- include('./blocks/component/notice', { artadvanced: site.customConfig.artadvanced }) %>
                <!-- [ 公告 ] end -->
                <!-- [清单] start -->
<!-- start: .articles -->
<div class="list-home list-grid list-grid-padding div-black-745" id="list-<%= site.customConfig.listform %>">
    <% posts.forEach(function(post, postIndex) { %>
    <% if ( post.isTop ) { %>
    <!-- 如果是置顶文章 -->
<%- include('./blocks/post-zd', { title: post.title, tags: post.tags, link: post.link, description: post.description, feature: post.feature, date: post.date, siteName: themeConfig.siteName }) %>
    <% }else{ %>
    <!-- 如果不是置顶文章 -->
<%- include('./blocks/post-list-zt', { title: post.title, tags: post.tags, link: post.link, description: post.description, feature: post.feature, date: post.date, siteName: themeConfig.siteName }) %>
<% } %>
    <% }); %>
</div>

       
<div class="pagenavi text-center"><a href="<%= pagination.next %>">加载更多</a></div>

<!-- 普通文章 -->
                <!-- [清单] end -->
            </div>
            <div class="col-md-4">
                <!-- [侧边栏] start -->
                <%- include('./blocks/component/sidebar', { mysocial: site.customConfig.mysocial }) %>
                <!-- [侧边栏] end -->
            </div>
        </div>
    </div>
    <!-- [ 版权 ] start -->
    <%- include('./blocks/component/foot') %>
    <!-- [ 版权 ] end -->
</section>
        <!--内容 over-->

    <!-- 尾部 -->
    <%- include('./blocks/footer') %>
    <!--尾部 over-->